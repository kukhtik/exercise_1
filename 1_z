package com.company;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {

	Scanner in = new Scanner(System.in);
	System.out.print("Input a number x: ");
	int x = in.nextInt();
	int a = 1, b = 2;
	double y = a * Math.tan(x) + b * Math.abs(x);
	System.out.print("f(x) = ");
	System.out.println(y);
	in.close();
    }
}
